

<div class="card-wrapper-container">
<div class="card-title-container">
<span class="card-title">
    Editing <%= @cat.name %>
</span>
</div>
<div class="card-main-container flex-1 flex-col p-4 justify-center items-center">
    <%= render 'form', cat: @cat %>
    <div class="mt-4">
        <%= link_to 'Show', @cat, class: "btn btn-yellow" %>
        <%= link_to 'Back', cats_path, class: "btn btn-purple" %>
    </div>
</div>
</div>